{
  "name": "CodeSentinel",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean"
  },
  "devDependencies": {
    "prettier": "^3.5.0",
    "turbo": "^2.4.1",
    "typescript": "5.7.3",
    "husky": "^8.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "husky": "^9.1.7"
  }
}
